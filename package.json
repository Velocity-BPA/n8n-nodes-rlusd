{
  "name": "n8n-nodes-rlusd",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Ripple's RLUSD stablecoin providing 18 resources and 60+ operations for multi-chain balance management, transfers, trustlines, DEX trading, compliance checking, and attestation verification on XRPL and Ethereum.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-rlusd.git"
  },
  "homepage": "https://velobpa.com",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "rlusd",
    "ripple",
    "xrpl",
    "xrp-ledger",
    "ethereum",
    "stablecoin",
    "cryptocurrency",
    "blockchain",
    "defi",
    "dex",
    "trustline",
    "cross-chain",
    "erc20",
    "payments",
    "compliance"
  ],
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "dist/nodes/Rlusd/Rlusd.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/RlusdNetwork.credentials.js",
      "dist/credentials/RippleApi.credentials.js",
      "dist/credentials/Custody.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Rlusd/Rlusd.node.js",
      "dist/nodes/Rlusd/RlusdTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "ethers": "^6.11.0",
    "ws": "^8.16.0",
    "xrpl": "^3.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
